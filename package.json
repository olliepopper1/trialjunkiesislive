  "scripts": {
    "start": "npm run build && node dist/index.js",
    "dev": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "test": "mocha",
    "test:watch": "NODE_ENV=test mocha --watch",
    "test:coverage": "NODE_ENV=test jest --config jest.config.cjs --coverage",
    "test:debug": "NODE_ENV=test mocha --config .mocharc.json --inspect-brk",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext .js,.cjs,.ts --fix",
    "test:unit": "mocha 'tests/unit/**/*.test.cjs'",
    "test:integration": "mocha 'tests/integration/**/*.test.cjs'",
    "test:e2e": "mocha 'tests/e2e/**/*.test.cjs'",
    "test:performance": "mocha 'tests/performance/**/*.test.cjs'",
    "test:security": "mocha 'tests/security/**/*.test.cjs'",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e && npm run test:performance && npm run test:security",
    "client": "cd client && npm start",
    "client:install": "cd client && npm install",
    "dev:full": "concurrently \"npm run dev\" \"npm run client\"",
    "preview": "vite preview"
  },
